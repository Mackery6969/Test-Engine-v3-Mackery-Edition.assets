import funkin.ui.story.Level;
import funkin.save.Save;

class Week5Level extends Level
{
	function new()
	{
		super('week5');
	}

  override function isUnlocked():Bool {
    return true;
  }

  override function getSongDisplayNames(difficultyId:String):Array<String> {
    if (Save.instance.hasBeatenLevel('week5')) {
      // Winter Horrorland is a secret song ;)
      return ['Cocoa', 'Eggnog', 'Winter Horrorland'];
    } else {
      return ["Cocoa", "Eggnog"];
    }
  }
}

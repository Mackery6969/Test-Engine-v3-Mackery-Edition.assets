import funkin.ui.story.Level;
import funkin.save.Save;

class Week2Level extends Level
{
	function new()
	{
		super('week2');
	}

  override function isUnlocked():Bool {
    return true;
  }

  override function getSongDisplayNames(difficultyId:String):Array<String> {
    if (Save.instance.hasBeatenLevel('week2')) {
      // Monster is a secret song ;)
      return ['Spookeez', 'South', 'Monster'];
    } else {
      return ["Spookeez", "South"];
    }
  }
}
